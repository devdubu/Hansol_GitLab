<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hansol.project.repository.HansolRepository">
    <select id="getMemberData" resultType="com.hansol.project.domain.HansolDomain">
        select
            *
        from
            HansolDB
    </select>
    <insert id="inputMemberData" resultType="com.hansol.project.domain.HansolDomain">
        insert into(
            *
        )
        value(
            email=#{email},
            password=#{password}
        )
    </insert>

</mapper>